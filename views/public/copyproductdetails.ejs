    <script>
      function showZoom() {
        const lens = document.querySelector('.img-zoom-lens');
        const result = document.querySelector('.img-zoom-result');
        lens.style.display = 'block';
        result.style.display = 'block';
      }
    
      function hideZoom() {
        const lens = document.querySelector('.img-zoom-lens');
        const result = document.querySelector('.img-zoom-result');
        lens.style.display = 'none';
        result.style.display = 'none';
      }
    
      function moveLens(event) {
        const lens = document.querySelector('.img-zoom-lens');
        const result = document.querySelector('.img-zoom-result');
        const img = document.getElementById('myimage');
        const cx = result.offsetWidth / lens.offsetWidth;
        const cy = result.offsetHeight / lens.offsetHeight;
        const x = event.offsetX - lens.offsetWidth / 2; // Adjust the x position
        const y = event.offsetY - lens.offsetHeight / 2; // Adjust the y position
        const bgPosX = -x * cx;
        const bgPosY = -y * cy;
        lens.style.left = x + 'px';
        lens.style.top = y + 'px';
        result.style.backgroundImage = `url('${img.src}')`;
        result.style.backgroundPosition = `${bgPosX}px ${bgPosY}px`;
      }
    </script>
