      svg.mail {
        width: 1.5rem;
        padding-right: 10px;
      }
      svg.message {
        width: 1.5rem;
        padding-right: 10px;
      }
      svg.bell {
        width: 1.5rem;
        padding-right: 10px;
      }
      svg.home {
        width: 1.5rem;
      }

      table.std {
        border-collapse: collapse;
        width: 100%;
      }
      table.std thead th {
        text-transform: uppercase;
        font-weight: 200;

        padding: 1em 0.5em;
        text-align: left;
      }

      table.std tbody tr {
        border-bottom: thin solid #ededed;
      }

      table.std tbody td img {
        width: 2.5rem;
        height: 2.5rem;
        border-radius: 50%;
        vertical-align: middle;
      }
      table.std tbody td span.name {
        color: #9f9f9f;
        font-weight: 700;

        padding-left: 1em;
      }

      @media screen and (max-width: 950px) {
        aside.left-sidebar {
          display: flex;
          width: 3.5rem;
          transition: width 0.1s ease-in-out;
        }
        aside.left-sidebar ul li span label {
          display: none;
        }

        aside.left-sidebar ul label {
          display: none;
        }
        main.main-content {
          width: 100%;
          margin: 0;
        }
      }
    </style>

