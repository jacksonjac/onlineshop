                    class="material-input__group__input iconic"
                    required
                    onkeyup="validateInput(this.value)"
                />
    
                <label class="material-input__group__label iconic">Password</label>
            </div>
            <error id="errorlog" style="color: rgb(248, 242, 242);"></error>
    
            <button type="submit" id="submitbutton" class="l-login__form__button">Login</button>
        </form>
        
        <% if (typeof message !== "undefined") { %>
            <p style="color: rgb(227, 236, 227)"><%= message %></p>
