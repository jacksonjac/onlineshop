    <script type="text/javascript">
      var pattern = /\s/g;
      var alertmsg = document.getElementById("errorlog"); // Use the correct id
      var submitButton = document.getElementById("submitbutton");
  
      function validateInput(data) {
          var isSpace = pattern.test(data);
          var isNegativeNumber = parseFloat(data) < 0; // Convert input to float and check if negative
  
          if (isSpace || isNegativeNumber) {
              alertmsg.innerHTML = "Invalid input";
              submitButton.style.display = "none"; // Hide the submit button
          } else {
              alertmsg.innerHTML = "";
              submitButton.style.display = "block"; // Show the submit button
          }
      }
  </script>
