  align-items: center;
  
  position:fixed;
  width:100%;
}

.page-header nav {
  display: flex;
}
.page-header > span {
  padding: 0 .5rem;
  text-transform: uppercase
}
.page-header img {
  width: 2rem;
  border-radius: .5rem;
}
.page-header > input {
   border-radius: var(--default-border-radius);
  padding: .5rem;
